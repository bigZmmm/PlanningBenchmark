  (oneof  (at y1) (at y2) (at y3) (at y4))
  (succ_loc x1 x2)
  (succ_loc x2 x3)
  (succ_loc x3 x4)
  (succ_loc y1 y2)
  (succ_loc y2 y3)
  (succ_loc y3 y4)
