  (prob_at x1 blocksworld)
	 (s_on c a x1) (s_on_table a x1) (s_on_table b x1)
  (s_clear b x1) (s_clear c x1) (s_empty x1)
  (prob_at x2 logistics)
  (s_pkg_at p1 l12 x2) (s_truck_at t1 l12 x2)
  (s_truck_at t2 l21 x2) (s_plane_at a1 l11 x2)
  (prob_at x3 logistics)
  (s_pkg_at p1 l12 x3) (s_truck_at t1 l12 x3)
  (s_truck_at t2 l21 x3) (s_plane_at a1 l11 x3)
